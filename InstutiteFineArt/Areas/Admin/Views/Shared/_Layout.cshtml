<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Diamond Stay</title>


    <!-- Bootstrap -->
    @Styles.Render("~/Admin/Index/css")
    @Styles.Render("~/Admin/Index2/css")
    <link href="~/App_Themes/CMS/vendors/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet">
    <link href="~/App_Themes/CMS/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/App_Themes/CMS/build/css/style.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" />
    <link href="~/App_Themes/Upload/css/fileinput.css" media="all" rel="stylesheet" type="text/css" />
    <link href="~/App_Themes/Upload/themes/explorer-fas/theme.css" media="all" rel="stylesheet" type="text/css" />

</head>
<body class="nav-md">

    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;">
                        <a href="/Home/Index" class="site_title"><i class="fa fa-paw"></i> <span>Institute of Fine Arts!</span></a>
                    </div>

                    <div class="clearfix"></div>
                    <!-- sidebar menu -->
                    @Html.Partial("_SidebarMenu")
                    <!-- /sidebar menu -->
                    <!-- /menu footer buttons -->
                    @Html.Partial("_PartialLogin")
                    <!-- /menu footer buttons -->
                </div>
            </div>

            <!-- top navigation -->
            @Html.Partial("_top_navigation")
            <!-- /top navigation -->
            <!-- page content -->
            <div class="right_col" role="main">
                @RenderBody()
            </div>
            <!-- /page content -->
            <!-- footer content -->
            <footer>
                <div class="pull-right">
                    Thiết kế bởi Diamond Stay!
                </div>
                <div class="clearfix"></div>
            </footer>
            <!-- /footer content -->
        </div>
    </div>
    <!-- jQuery -->

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.0/jquery.min.js"></script>
    <script src="~/App_Themes/CMS/vendors/raphael/raphael.min.js"></script>
    <script src="~/App_Themes/CMS/vendors/morris.js/morris.min.js"></script>
 


    @Scripts.Render("~/Admin/Index/jquery")
    @Scripts.Render("~/Admin/Index2/jquery")

    <script src="~/App_Themes/Upload/js/fileinput.js" type="text/javascript"></script>
    <script src="~/App_Themes/Upload/js/plugins/piexif.js" type="text/javascript"></script>
    <script src="~/App_Themes/Upload/js/plugins/sortable.js" type="text/javascript"></script>
    <script src="~/App_Themes/Upload/js/fileinput.js" type="text/javascript"></script>
    <script src="~/App_Themes/Upload/js/locales/fr.js" type="text/javascript"></script>
    <script src="~/App_Themes/Upload/js/locales/es.js" type="text/javascript"></script>
    <script src="~/App_Themes/Upload/themes/fas/theme.js" type="text/javascript"></script>
    <script src="~/App_Themes/Upload/themes/explorer-fas/theme.js" type="text/javascript"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
